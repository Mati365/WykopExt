{
    "manifest_version": 2
  , "name": "WykopExt"
  , "description": "A simple browser extension that shows notifications from wykop.pl"
  , "version": "0.0.6"
  , "browser_action": {
      "default_icon": "data/icon.png"
    , "default_popup": "data/popups/popup.html"
  }
  , "icons": {
        "16": "data/icon.png"
      , "48": "data/icon.png"
      , "128": "data/icon.png"
  }
  , "background": {
      "scripts": [
          "bower_components/jquery/dist/jquery.min.js"
        , "bower_components/cryptojslib/rollups/md5.js"
        , "bower_components/cryptojslib/components/md5-min.js"
        , "bower_components/underscore/underscore-min.js"
        , "js/background.js"
      ]
  }
  , "web_accessible_resources": [
      "data/popups/login.html"
  ]
  , "permissions": [
        "<all_urls>"
      , "tabs"
      , "storage"
      , "unlimitedStorage"
  ]
}